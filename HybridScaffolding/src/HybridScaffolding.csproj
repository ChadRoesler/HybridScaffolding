<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ApplicationIcon>Resouces\Icons\HybridScaffolding.ico</ApplicationIcon>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.15.0</FileVersion>
    <Version>1.0.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIconUrl>https://raw.githubusercontent.com/ChadRoesler/HybridScaffolding/master/HybridScaffolding/src/Resouces/Icons/HybridScaffolding.ico</PackageIconUrl>
    <PackageProjectUrl>https://github.com/ChadRoesler/HybridScaffolding</PackageProjectUrl>
    <PackageReleaseNotes>Conversion to .net standard</PackageReleaseNotes>
    <Authors>Chad Roesler</Authors>
    <Description>Used for preping Hybrid applications (cli/gui applications).</Description>
    <PackageTags>Console CommandLine Forms HybridApplication Powershell DotNetCore NetStandard MVC</PackageTags>
    <Copyright>2016</Copyright>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Release\HybridScaffolding.xml</DocumentationFile>
    <OutputPath>bin\Release\netstandard2.0</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>bin\Release\netstandard2.0\</OutputPath>
    <DocumentationFile>bin\Release\netstandard2.0\HybridScaffolding.xml</DocumentationFile>
  </PropertyGroup>

</Project>
